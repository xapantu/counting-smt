(set-info :smt-lib-version 2.0)
(set-logic QF_LIRA)
(declare-fun N_PID () Int)
(assert (>= N_PID 2))
(declare-range PID (0 N_PID))
(declare-fun b () (Array PID Bool))
(declare-fun a () (Array PID Bool))
(assert (= (# ((z PID)) (or (select b z) (select a z))) N_PID) )
(assert (not (= (# ((z PID)) (or (select b z) (select a z ))) N_PID)))
(check-sat)
(check-sat)
