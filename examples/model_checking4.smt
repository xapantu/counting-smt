(set-logic QF_ALIRA)
(declare-fun N_PID () Int)
(declare-range PID (0 N_PID))
(declare-fun |s0.cx| () (Array PID Bool))
(declare-fun |s0.good_p| () (Array PID Bool))
(assert (= (select |s0.cx| 0) true) )
(assert (> N_PID 0))
(assert (and (= (# ((|var!!0| PID)) (=> (select |s0.good_p| |var!!0|) (= (select |s0.cx| |var!!0|) (select |s0.cx| 0)))) N_PID)))
(assert (and (select |s0.good_p| 0) (< (# ((|var!!0| PID)) (= (select |s0.cx| |var!!0|) true)) N_PID)))
(check-sat)
